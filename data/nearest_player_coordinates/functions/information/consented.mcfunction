#> This function is triggered from information

tellraw @s {"text":"===== Select Target Priority =====","color":"gold"}
tellraw @s {"text":"[Opt-out] - /trigger npc.toggle","color":"red","clickEvent":{"action":"run_command","value":"/trigger npc.toggle set 1"},"hoverEvent":{"action":"show_text","contents":[{"text":"Opt out of coordinate sharing, so you won't see other's coordinates and others can't see yours either"}]}}
tellraw @s {"text":"[None] - /trigger npc.priority set 0","color":"aqua","clickEvent":{"action":"run_command","value":"/trigger npc.priority set 0"},"hoverEvent":{"action":"show_text","value":{"text":"Track nobody!"}}}
tellraw @s {"text":"[Nearest] - /trigger npc.priority set 1","color":"aqua","clickEvent":{"action":"run_command","value":"/trigger npc.priority set 1"},"hoverEvent":{"action":"show_text","value":{"text":"Track the nearest player!"}}}
tellraw @s {"text":"[Furthest] - /trigger npc.priority set 2","color":"aqua","clickEvent":{"action":"run_command","value":"/trigger npc.priority set 2"},"hoverEvent":{"action":"show_text","value":{"text":"Track the furthest player!"}}}
tellraw @s {"text":"See who's tracking you (Coming Soon!)","color":"blue"}
tellraw @s {"text":"===== Or Select a player to track =====","color":"gold"}
execute if entity @a[tag=npc.player3,limit=1] run tellraw @s ["",{"text":"["},{"selector":"@a[tag=npc.player3]","color":"aqua","clickEvent":{"action":"run_command","value":"/trigger npc.priority set 3"},"hoverEvent":{"action":"show_text","contents":[{"text":"Track this player!"}]}},{"text":"] - /trigger npc.priority set 3"}]
execute if entity @a[scores={npc.track_id=4},limit=1] run tellraw @s ["",{"text":"["},{"selector":"@a[scores={npc.track_id=4},limit=1]","color":"aqua","clickEvent":{"action":"run_command","value":"/trigger npc.priority set 4"},"hoverEvent":{"action":"show_text","contents":[{"text":"Track this player!"}]}},{"text":"] - /trigger npc.priority set 4"}]
execute unless entity @a[scores={npc.track_id=4}] run tellraw @s {"text":"[Track me] - /trigger npc.track_id set 4","color":"aqua","clickEvent":{"action":"run_command","value":"/trigger npc.track_id set 4"},"hoverEvent":{"action":"show_text","contents":[{"text":"Inserts your name here so that other players can view your coordinates"}]}}
